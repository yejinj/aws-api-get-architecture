# AWS 기본 설정
AWS_REGION=us-east-2
AWS_PROFILE=default

# 서버 설정
SERVER_PORT=8080
CONTEXT_PATH=/api

# 로깅 설정
LOG_LEVEL_ROOT=INFO
LOG_LEVEL_APP=DEBUG

# EC2 설정 (예시)
EC2_INSTANCE_ID=i-xxxxxxxxxxxxxxxxx
EC2_INSTANCE_NAME=your-instance-name
EC2_INSTANCE_TYPE=t3.micro
EC2_AMI_ID=ami-xxxxxxxxxxxxxxxxx
EC2_PLATFORM=Linux/UNIX
EC2_VCPU_COUNT=2

# RDS 설정 (예시)
RDS_INSTANCE_ID=your-database-instance
RDS_MASTER_USERNAME=admin
RDS_MASTER_PASSWORD=your-password
RDS_ENDPOINT=your-database.region.rds.amazonaws.com
RDS_PORT=3306
RDS_ENGINE=mysql
RDS_AVAILABILITY_ZONE=us-east-2c
RDS_VPC_ID=vpc-xxxxxxxxxxxxxxxxx
RDS_SUBNET_GROUP=default-vpc-xxxxxxxxxxxxxxxxx
RDS_SECURITY_GROUP_1=sg-xxxxxxxxxxxxxxxxx
RDS_SECURITY_GROUP_2=sg-xxxxxxxxxxxxxxxxx
RDS_PUBLICLY_ACCESSIBLE=false

# CloudWatch 설정
CLOUDWATCH_DEFAULT_PERIOD=300
CLOUDWATCH_MAX_DATAPOINTS=1440

# S3 설정 (향후 사용)
S3_DEFAULT_BUCKET=your-bucket-name
S3_MAX_KEYS=1000

# Lambda 설정 (향후 사용)
LAMBDA_DEFAULT_FUNCTION_NAME=your-function-name
LAMBDA_MAX_ITEMS=50
