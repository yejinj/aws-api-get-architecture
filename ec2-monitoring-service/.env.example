# AWS 설정 (예시)
AWS_REGION=us-east-2
AWS_PROFILE=default

# EC2 인스턴스 정보 (본인의 인스턴스 정보로 변경하세요)
EC2_INSTANCE_ID=i-xxxxxxxxxxxxxxxxx
EC2_INSTANCE_NAME=your-instance-name
EC2_INSTANCE_TYPE=t3.micro
EC2_AMI_ID=ami-xxxxxxxxxxxxxxxxx
EC2_PLATFORM=Linux/UNIX
EC2_VCPU_COUNT=2

# CloudWatch 설정
CLOUDWATCH_DEFAULT_PERIOD=300
CLOUDWATCH_MAX_DATAPOINTS=1440

# 서버 설정
SERVER_PORT=8080
CONTEXT_PATH=/api

# 로깅 레벨
LOG_LEVEL_ROOT=INFO
LOG_LEVEL_APP=DEBUG
